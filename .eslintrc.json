{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-undef": "error"
    },
    "globals": {
        "document": "readonly",
        "window": "readonly",
        "navigator": "readonly",
        "getComputedStyle": "readonly"
    }
}